.index_swiper swiper {
  width: 750rpx;
  height: 410rpx;
}
.index_swiper swiper image {
  width: 100%;
}
.index_cate {
  display:flex; 
  flex-wrap:wrap;
  width: 100%;
  height: 600rpx;
}
.index_cate navigator {
  padding:20rpx;
  width: 50%;
  height: 300rpx;
  font-size: 30rpx;
  flex-direction: column;
  text-align: center;
  display: flex;
  justify-content: center;
  align-items: center;
}
.index_cate navigator image{
  border-radius: 20rpx;
}
